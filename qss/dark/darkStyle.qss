/*QPushButton*/
QPushButton{
    color:#EEEEEE;
    background-color:#0192e3;
    border-style:outset;
    border-width:2px;
    border-color:#F4F4F4;
    border-radius:10px;
    font:bold 14px;
    font-family: Segoe UI;
    min-width:100px;
    min-height:20px;
    padding:4px;
}
QPushButton:normal{
    color:#EEEEEE;
    background-color:#0192e3;
    border-style:inset;
    padding-left:4px;
    padding-top:4px;
}
QPushButton:hover{
    color:#FFFFFF;
    background-color:#0870b9;
    border-style:inset;
    padding-left:6px;
    padding-top:6px;
}
QPushButton:pressed{
    color:#000000;
    background-color:#023086;
    border-style:inset;
    padding-left:6px;
    padding-top:6px;
}
QPushButton:disabled{
    color:#000000;
    background-color:#808080;
    border-style:inset;
    padding-left:6px;
    padding-top:6px;
}
QPushButton:flat{
    border:2px solid red;
}
