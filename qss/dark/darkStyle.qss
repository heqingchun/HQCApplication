/*QListView*/
QListView{
    color:black;
    background-color:#a0d8ff;
    border-style:outset;
    border-width:1px;
    border-color:#585858;
    border-radius:10px;
    font:bold 15px;
    min-width:100px;
    min-height:20px;
    padding:5px;
}
QListView::item:selected{
    border: 1px solid #6a6ea9;
}
QListView::item:selected:!active{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #585858, stop: 1 #e7e7e7);
}
QListView::item:selected:active{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #bb44ff, stop: 1 #bb44ff);
}
QListView::item:hover{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #ff99ee, stop: 1 #ff00d4);
    border-style:inset;
    padding-left:3px;
    padding-top:1px;
}
/*QProgressBar*/
QProgressBar{
    height:22px;
    text-align:center;
    font-size:14px;
    color:white;
    border-radius:11px;
    background:#032a79;
}
QProgressBar::chunk{
    border-radius:11px;
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 #99ff33,stop:1 #11ff00);
}
/*QGroupBox */
QGroupBox {
    background-color:#a0d8ff;
    border: 2px solid #EEEEEE;
    border-radius: 5px;
    margin-top: 20px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 8px 8px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #a0d8ff, stop: 1 #a0d8ff);
    border-radius: 5px;
}
QGroupBox::indicator {
    width: 13px;
    height: 13px;
}
QGroupBox::indicator:unchecked {
    image: url(:/images/checkbox.png);
}
/*QLCDNumber*/
QLCDNumber{
    color:#FFFFFF;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #0ED145, stop: 1 #585858);
    border: 2px solid #EEEEEE;
    border-radius: 5px;
}
